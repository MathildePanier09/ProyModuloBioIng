            <Button fx:id="salirBttn" mnemonicParsing="false" onAction="#salirAction" styleClass="boton2" stylesheets="@application.css" text="salir " textFill="#a04ba3" GridPane.rowIndex="2">
               <font>
                  <Font size="23.0" />
               </font>
               <GridPane.margin>
                  <Insets left="240.0" />
               </GridPane.margin></Button>
         </children>
      </GridPane>
   </children>
</AnchorPane>
