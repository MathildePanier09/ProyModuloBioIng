<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="body" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane layoutX="-4.0" prefHeight="402.0" prefWidth="607.0">
