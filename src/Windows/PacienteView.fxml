          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label stylesheets="@application.css" text="Paciente id: " textFill="#a04ba3">
               <font>
                  <Font size="31.0" />
               </font>
               <GridPane.margin>
                  <Insets left="200.0" />
